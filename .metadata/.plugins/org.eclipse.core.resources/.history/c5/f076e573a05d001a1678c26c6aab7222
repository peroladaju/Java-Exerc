		//y = 2 * x;
		
		//System.out.println(x);
		//System.out.println(y);
		
		
		double b, B, h, area;
		//o zero depois do valor indica que é um número do tipo double, se for float finalizar com f.
		b = 6.0;
		B = 8.0;
		h = 5.0;
		area = (b + B)/2.0 * h;
		
		System.out.println(area);
		
		int a, b, result;
		a = 5;
		b = 2;
		result = a / b;
		System.out.println(result);
		
	}

