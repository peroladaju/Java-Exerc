package br.com.juju.cursoexercicio;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
	Scanner leitor = new Scanner(System.in);
	
	int x, y, result;
	
	x = leitor.nextInt();
	y = leitor.nextInt();
	
	result = x + y;
	System.out.println("A soma dos valores = " + result);
	
	
	leitor.close();
	}

}
